mmendationClick = () => {\n      console.log(\"AI 추천 버튼 클릭됨\");\n      // AI 추천 로직을 여기에 구현\n    };\n\n    return {\n      modalPopupStatue,\n      searchInput,\n      filterTypes,\n      educationCount,\n      educationList,\n      selectedProvince,\n      searchEducation,\n      onMoveDetailPageClick,\n      onFilterClick,\n      handleSelectProvince,\n      handleAIRecommendationClick,\n    };\n  }\n}\n</script>\n\n<style scoped>\n.body {\n  flex: 1;\n  padding: 20px;\n  box-sizing: border-box;\n  background-color: #f8f9fa;\n  overflow-y: auto;\n}\n\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 20px;\n}\n\n.header-title {\n  font-size: 24px;\n  font-weight: bold;\n}\n\n.header-ai-recommendation {\n  background-color: #007BFF;\n  color: white;\n  padding: 10px 15px;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\n.recommended-section {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.recommended-content {\n  background-color: #D9D9D9;\n  border-radius: 15px;\n  padding: 15px;\n  height: 150px;\n  position: relative;\n}\n\n.ai-recommendation {\n  background-color: #007BFF;\n  color: white;\n  padding: 10px 15px;\n  border-radius: 5px;\n  text-align: center;\n  cursor: pointer;\n  position: absolute;\n  top: 10px; /* Adjust based on your design */\n  right: 10px; /* Adjust based on your design */\n}\n\n.search-wrap {\n  margin-top: 15px;\n  margin-bottom: 15px;\n}\n\n.search-input {\n  font-size: 16px;\n  width: 100%;\n  padding: 10px 30px 10px 10px;\n  border-radius: 8px;\n  border: 1px solid #413F42;\n  box-sizing: border-box;\n  background-image: url('@/assets/images/icons/search.png');\n  background-size: 20px 20px;\n  background-repeat: no-repeat;\n  background-position: right 10px center;\n}\n\n.filter-section {\n  display: flex;\n  justify-content: space-between;\n  margin-top: 20px;\n}\n\n.filter-item {\n  flex: 1;\n  text-align: center;\n  font-size: 16px;\n  padding: 10px;\n  border: 1px solid #413F42;\n  border-radius: 8px;\n  background-color: #ffffff;\n  height: 16px;\n  margin-right: 10px;\n  cursor: pointer;\n}\n\n.filter-item:last-child {\n  margin-right: 0;\n}\n\n.job-posting-wrap {\n  margin-top: 10px;\n}\n\n.job-posting-info {\n  font-size: 20px;\n  font-weight: bold;\n  margin-left: 10px;\n  margin-bottom: 10px;\n}\n\n.job-posting-list {\n  background-color: #D9D9D9;\n  border-radius: 15px;\n  padding: 15px;\n  color: #000000;\n  font-weight: bold;\n  margin-bottom: 5px;\n}\n\n.course-title {\n  font-size: 18px;\n  margin-right: auto;\n}\n\n.course-subtitle {\n  margin-bottom: 5px;\n}\n\n.course-schedule {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.schedule-info {\n  flex: 1;\n}\n\n.right-arrow-icon {\n  width: 20px;\n  height: auto;\n  cursor: pointer;\n}\n\n.recommended-title {\n  font-size: 20px;\n  font-weight: bold;\n  margin-left: 10px;\n  margin-bottom: 10px;\n}\n</style>\n"],"mappings":";AAsDA,SAASA,SAAS,EAAEC,GAAE,QAAS,KAAK;AACpC,SAASC,UAAS,QAAS,gBAAgB;AAC3C,SAASC,MAAK,QAAS,iBAAiB;AACxC,SAASC,SAAQ,QAAS,YAAY;AACtC,OAAOC,iBAAgB,MAAO,oCAAoC;AAClE,SAASC,mBAAkB,QAAS,+BAA+B;AAEnE,eAAe;EACbC,IAAI,EAAE,sBAAsB;EAC5BC,QAAQ,EAAE;IACRF,mBAAmBA,CAAA,EAAG;MACpB,OAAOA,mBAAmB;IAC5B;EACF,CAAC;EACDG,UAAU,EAAE;IAAEJ;EAAkB,CAAC;EACjCK,OAAO,EAAE;IAAER;EAAW,CAAC;EACvBS,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIR,SAAS,CAAC,CAAC;IAC1B,MAAMS,gBAAe,GAAIZ,GAAG,CAAC,KAAK,CAAC;IACnC,MAAMa,WAAU,GAAIb,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMc,WAAU,GAAId,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMe,cAAa,GAAIf,GAAG,CAAC,CAAC,CAAC;IAC7B,MAAMgB,aAAY,GAAIhB,GAAG,CAAC,EAAE,CAAC;IAC7B,MAAMiB,gBAAe,GAAIjB,GAAG,CAAC,OAAO,CAAC;IAErC,MAAMkB,eAAc,GAAIA,CAAA,KAAM;MAC5BC,OAAO,CAACC,GAAG,CAAC,MAAM,EAAEP,WAAW,CAACQ,KAAK,CAAC;IACxC,CAAC;IAED,MAAMC,cAAa,GAAIA,CAAA,KAAM;MAC3B,MAAMC,IAAG,GAAI,CACX;QACEC,KAAK,EAAE,CAAC;QACRC,OAAO,EAAE,QAAQ;QACjBC,OAAO,EAAE,sBAAsB;QAC/BC,MAAM,EAAE,KAAK;QACbC,MAAM,EAAEC,OAAO,CAAC,2CAA2C,CAAC;QAC5DC,SAAS,EAAE,YAAY