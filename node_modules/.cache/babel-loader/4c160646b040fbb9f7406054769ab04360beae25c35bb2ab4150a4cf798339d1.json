{"ast":null,"code":"import ModalPopup from \"@/components/SingleButtonModal.vue\";\nimport { ROUTES } from \"@/router/routes\";\nimport { ref } from \"vue\";\nexport default {\n  name: \"JobPostingRegister\",\n  computed: {\n    ROUTES() {\n      return ROUTES;\n    }\n  },\n  components: {\n    ModalPopup\n  },\n  setup() {\n    const modalPopupStatue = ref(false);\n    const onRegisterClick = () => {\n      modalPopupStatue.value = true;\n    };\n    const onImageRegisterClick = () => {};\n    return {\n      modalPopupStatue,\n      onRegisterClick,\n      onImageRegisterClick\n    };\n  }\n};","map":{"version":3,"names":["ModalPopup","ROUTES","ref","name","computed","components","setup","modalPopupStatue","onRegisterClick","value","onImageRegisterClick"],"sources":["/Users/sol/Desktop/fisa/wooribound-web/src/views/CorporateUser/JobPostingRegister.vue"],"sourcesContent":["<script>\nimport ModalPopup from \"@/components/SingleButtonModal.vue\";\nimport {ROUTES} from \"@/router/routes\";\nimport {ref} from \"vue\";\n\nexport default {\n  name: \"JobPostingRegister\",\n  computed: {\n    ROUTES() {\n      return ROUTES\n    }\n  },\n  components: {ModalPopup},\n  setup() {\n    const modalPopupStatue = ref(false);\n\n    const onRegisterClick = () => {\n      modalPopupStatue.value = true;\n    }\n\n    const onImageRegisterClick = () => {\n\n    }\n\n    return {\n      modalPopupStatue,\n      onRegisterClick,\n      onImageRegisterClick,\n    };\n  }\n}\n</script>\n\n<template>\n  <main class=\"body\">\n    <div class=\"header\">공고 관리</div>\n    <div class=\"subtitle\">공고 등록</div>\n    <div class=\"content\">\n      <div class=\"input-section\">\n        <!-- 공고 이미지 등록 -->\n        <div class=\"input-label\">\n          <span class=\"required\">*</span>\n          <input\n              class=\"input-field\"\n              type=\"file\"\n              ref=\"imageInputRef\"\n              @change=\"onImageRegisterClick\"\n              accept=\"image/*\"\n              style=\"display: none;\"\n          />\n          <input\n              class=\"input-field\"\n              placeholder=\"공고사진\"\n              readonly\n              :value=\"imageUrl ? imageUrl.split('/').pop() : ''\" \n          />\n          <button class=\"check-button\" @click=\"imageInputRef.value.click()\">사진등록</button>\n        </div>\n        <!-- 공고제목 입력  -->\n        <div class=\"input-label\">\n          <span class=\"required\">*</span>\n          <input class=\"input-field\" placeholder=\"공고 제목\">\n        </div>\n        <!-- 공고 시작 일자 입력 -->\n        <div class=\"input-label\">\n          <span class=\"required\">*</span>\n          <input class=\"input-field\" type=\"date\" data-placeholder=\"공고 시작 일자\" required/>\n        </div>\n        <!-- 공고 시작 마감 입력 -->\n        <div class=\"input-label\">\n          <span class=\"required\">*</span>\n          <input class=\"input-field\" type=\"date\" data-placeholder=\"공고 마감 일자\" required/>\n        </div>\n        <div class=\"job-register-button\" @click=\"onRegisterClick\">공고 등록</div>\n      </div>\n    </div>\n  </main>\n  <modal-popup\n      v-if=\"modalPopupStatue\"\n      @close-modal=\"modalPopupStatue = false\"\n      :modal-message=\"'공고 등록이 완료 되었습니다.'\"\n      :router-path=\"ROUTES.JOB_POSTING_MANAGEMENT.path\"\n  />\n</template>\n\n<style scoped>\n.body {\n  flex: 1;                      /* 가변 영역, 헤더와 바텀 내비게이션을 제외한 나머지 공간 차지 */\n  padding: 20px;                /* 내부 여백 */\n  box-sizing: border-box;       /* 패딩을 포함한 크기 계산 */\n  background-color: #f8f9fa;    /* 배경색 */\n  overflow-y: auto;             /* 내용이 넘칠 경우 스크롤 가능 */\n}\n\n.header {\n  font-size: 24px;              /* 헤더 폰트 크기 */\n  font-weight: bold;            /* 헤더 두껍게 */\n  margin-bottom: 20px;          /* 아래쪽 여백 */\n}\n\n.subtitle {                     /* 소제목 스타일 */\n  font-size: 18px;              /* 글자 크기 */\n  font-weight: bold;            /* 글자 두께 */\n  margin-bottom: 10px;          /* 아래쪽 여백 */\n  color: #333;                  /* 텍스트 색상 */\n}\n\n.content {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: space-between; /* 콘텐츠와 요소를 공간을 균등하게 분배 */\n  height: 100%; /* 전체 높이를 사용하도록 설정 */\n}\n\n.input-section {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  width: 100%;\n  flex: 1; /* 가변적으로 공간을 차지하도록 설정 */\n}\n\n.input-label {\n  display: flex; /* 수평 정렬 */\n  align-items: center; /* 세로 중앙 정렬 */\n  width: 100%;\n}\n\n.input-field {\n  width: 100%; /* 너비를 100%로 설정하여 부모 요소의 전체 너비를 차지 */\n  padding: 10px;\n  margin: 10px 10px;\n  box-sizing: border-box;\n  font-size: 16px;\n  border-radius: 12px;\n  border: 1px solid #e1e1e1;\n  color: #413F42;\n}\n\n.required {\n  color: #F60F0F; /* 필수 입력 사항 색상 */\n  margin-right: 5px; /* 입력 필드와의 간격 */\n}\n\n.job-register-button {\n  width: 90%;\n  padding: 10px;\n  margin-top: 20px;\n  background-color: #024CAA;\n  color: white;\n  text-align: center;\n  cursor: pointer;\n  font-weight: bold;\n  border-radius: 8px;\n}\n\n.job-category-section {\n  display: flex;\n  flex-direction: column;\n  width: 100%;\n}\n\n.job-category {\n  display: flex; /* 수평 정렬 */\n  align-items: center; /* 세로 중앙 정렬 */\n  justify-content: space-between; /* 좌우 간격을 균등하게 */\n  margin-top: 10px;\n  height: 50px; /* 고정 높이 */\n  width: 100%;\n  margin-left: 22px;\n}\n\n.add-button img,\n.remove-button img {\n  width: 24px;\n  height: 24px;\n}\n\ninput[type=\"date\"] {\n  position: relative;\n  padding-right: 10px;\n  background: url(\"@/assets/images/icons/calender.png\") no-repeat right 10px center / 25px auto, #fff;\n  border: 1px solid #e1e1e1;\n  border-radius: 12px;\n}\n\ninput[type=\"date\"]::-webkit-calendar-picker-indicator {\n  position: absolute;\n  left: 0;\n  top: 0;\n  width: 100%;\n  height: 100%;\n  background: transparent;\n  color: transparent;\n  cursor: pointer;\n}\ninput[type=\"date\"]::before {\n  content: attr(data-placeholder);\n  width: 100%;\n}\ninput[type=\"date\"]:valid::before {\n  display: none;\n}\n\n.check-button {\n  padding: 0 15px; /* 상하 패딩을 0으로 설정 */\n  background-color: #024CAA; /* 버튼 배경색 */\n  color: white; /* 글자색 */\n  border: none; /* 테두리 없앰 */\n  border-radius: 12px; /* 버튼 모서리 둥글게 */\n  cursor: pointer; /* 커서 포인터로 변경 */\n  margin-left: 10px; /* 아이디 입력 필드와 버튼 간격 */\n  font-size: 14px; /* 글자 크기 */\n  height: 40px; /* 버튼 높이를 아이디 입력 필드와 동일하게 설정 */\n  white-space: nowrap; /* 텍스트가 줄 바꿈되지 않도록 설정 */\n  margin-right: 10px;\n}\n</style>"],"mappings":"AACA,OAAOA,UAAS,MAAO,oCAAoC;AAC3D,SAAQC,MAAM,QAAO,iBAAiB;AACtC,SAAQC,GAAG,QAAO,KAAK;AAEvB,eAAe;EACbC,IAAI,EAAE,oBAAoB;EAC1BC,QAAQ,EAAE;IACRH,MAAMA,CAAA,EAAG;MACP,OAAOA,MAAK;IACd;EACF,CAAC;EACDI,UAAU,EAAE;IAACL;EAAU,CAAC;EACxBM,KAAKA,CAAA,EAAG;IACN,MAAMC,gBAAe,GAAIL,GAAG,CAAC,KAAK,CAAC;IAEnC,MAAMM,eAAc,GAAIA,CAAA,KAAM;MAC5BD,gBAAgB,CAACE,KAAI,GAAI,IAAI;IAC/B;IAEA,MAAMC,oBAAmB,GAAIA,CAAA,KAAM,CAEnC;IAEA,OAAO;MACLH,gBAAgB;MAChBC,eAAe;MACfE;IACF,CAAC;EACH;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}