N;\n\n    const isAdmin = computed(() => {\n      return userStore.userType === USER_TYPES.INFRA_ADMIN || userStore.userType === USER_TYPES.SERVICE_ADMIN;\n    });\n\n    return {\n      isAdmin,\n      iconState,\n      fullScreenMenuState,\n      handleIconStateChanged,\n      fullScreenMenuStateChanged,\n    };\n  }\n}\n</script>\n\n<style scoped>\n.app-container {\n  display: flex;                /* Flexbox 사용 */\n  flex-direction: column;       /* 세로 방향 정렬 */\n  min-height: 100vh;           /* 최소 높이 전체 화면 */\n}\n\n/* App.vue 또는 main.css (전역 스타일 파일) */\na.router-link {\n  all: unset;\n  cursor: pointer;\n}\n</style>\n"],"mappings":"AAoBA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAK;AACnC,OAAOC,gBAAe,MAAO,mCAAmC;AAChE,OAAOC,eAAc,MAAO,kCAAkC;AAC9D,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,cAAa,MAAO,mCAAmC;AAC9D,SAASC,YAAW,QAAS,oBAAoB;AACjD,SAASC,UAAS,QAAS,uBAAuB;AAElD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ,cAAc;IACdD,cAAc;IACdD,eAAe;IACfD;EACF,CAAC;EACDQ,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIX,GAAG,CAAC,CAAC,CAAC,CAAC;IACzB,MAAMY,mBAAkB,GAAIZ,GAAG,CAAC,KAAK,CAAC;IAEtC,MAAMa,sBAAqB,GAAKC,QAAQ,IAAK;MAC3CH,SAAS,CAACI,KAAI,GAAID,QAAQ;MAC1BE,OAAO,CAACC,GAAG,CAAC,YAAY,EAAEN,SAAS,CAACI,KAAK,CAAC;IAC5C,CAAC;IAED,MAAMG,0BAAyB,GAAKJ,QAAQ,IAAK;MAC/CF,mBAAmB,CAACG,KAAI,GAAID,QAAQ;MACpCE,OAAO,CAACC,GAAG,CAAC,sBAAsB,EAAEL,mBAAmB,CAACG,KAAK,CAAC;IAChE,CAAC;;IAED;IACA,MAAMI,SAAQ,GAAIb,YAAY,CAAC,CAAC;IAChCa,SAAS,CAACC,UAAS,GAAI,IAAI;IAC3B;IACA;IACA;IACA;;IAEA,MAAMC,OAAM,GAAIpB,QAAQ,CAAC,MAAM;MAC7B,OAAOkB,SAAS,CAACG,QAAO,KAAMf,UAAU,CAACgB,WAAU,IAAKJ,SAAS,CAACG,QAAO,KAAMf,UAAU,CAACiB,aAAa;IACzG,CAAC,CAAC;IAEF,OAAO;MACLH,OAAO;MACPV,SAAS;MACTC,mBAAmB;MACnBC,sBAAsB;MACtBK;IACF,CAAC;EACH;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}