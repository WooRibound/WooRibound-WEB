{"ast":null,"code":"import { computed } from 'vue';\nimport { useUserStore } from \"@/stores/userStore\";\nimport { USER_TYPES } from \"@/constants/userTypes\";\nexport default {\n  name: \"AdminMain\",\n  setup() {\n    const userStore = useUserStore();\n    const titleText = computed(() => {\n      switch (userStore.userType) {\n        case USER_TYPES.SERVICE_ADMIN:\n          return \"서비스 관리자 입니다.\";\n        case USER_TYPES.INFRA_ADMIN:\n          return \"인프라 관리자 입니다.\";\n        default:\n          return \"알 수 없는 사용자입니다.\";\n      }\n    });\n    const imageSource = computed(() => {\n      switch (userStore.userType) {\n        case USER_TYPES.SERVICE_ADMIN:\n          return require(\"@/assets/images/icons/service_admin.png\");\n        case USER_TYPES.INFRA_ADMIN:\n          return require(\"@/assets/images/icons/infra_admin.png\");\n        default:\n          return require(\"@/assets/images/icons/unknown_admin.png\");\n      }\n    });\n    return {\n      titleText,\n      imageSource\n    };\n  }\n};","map":{"version":3,"names":["computed","useUserStore","USER_TYPES","name","setup","userStore","titleText","userType","SERVICE_ADMIN","INFRA_ADMIN","imageSource","require"],"sources":["/Users/sol/Desktop/fisa/wooribound-web/src/views/main/AdminMain.vue"],"sourcesContent":["<script>\nimport { computed } from 'vue';\nimport { useUserStore } from \"@/stores/userStore\";\nimport { USER_TYPES } from \"@/constants/userTypes\";\n\nexport default {\n  name: \"AdminMain\",\n  setup() {\n    const userStore = useUserStore();\n\n    const titleText = computed(() => {\n      switch (userStore.userType) {\n        case USER_TYPES.SERVICE_ADMIN:\n          return \"서비스 관리자 입니다.\";\n        case USER_TYPES.INFRA_ADMIN:\n          return \"인프라 관리자 입니다.\";\n        default:\n          return \"알 수 없는 사용자입니다.\";\n      }\n    });\n\n    const imageSource = computed(() => {\n      switch (userStore.userType) {\n        case USER_TYPES.SERVICE_ADMIN:\n          return require(\"@/assets/images/icons/service_admin.png\");\n        case USER_TYPES.INFRA_ADMIN:\n          return require(\"@/assets/images/icons/infra_admin.png\");\n        default:\n          return require(\"@/assets/images/icons/unknown_admin.png\");\n      }\n    });\n\n    return {\n      titleText,\n      imageSource,\n    };\n  },\n};\n</script>\n\n<template>\n  <main class=\"body\">\n    <div>\n      <div class=\"admin_title\">{{ titleText }}</div>\n      <img class=\"admin_image\" :src=\"imageSource\" v-if=\"imageSource\" />\n    </div>\n  </main>\n</template>\n\n<style scoped>\n.body {\n  flex: 1;                      /* 가변 영역, 헤더와 바텀 내비게이션을 제외한 나머지 공간 차지 */\n  padding: 20px;                /* 내부 여백 */\n  box-sizing: border-box;       /* 패딩을 포함한 크기 계산 */\n  background-color: #f8f9fa;    /* 배경색 */\n  overflow-y: auto;             /* 내용이 넘칠 경우 스크롤 가능 */\n}\n\n.admin_title {\n  font-weight: bold;            /* 굵은 글씨체 */\n  font-size: 24px;              /* 폰트 크기 설정 (조정 가능) */\n  margin-bottom: 20px;          /* 텍스트와 이미지 사이 여백 */\n}\n\n.admin_image {\n  margin-left: auto;\n  margin-right: auto;\n  width: 200px;\n}\n</style>"],"mappings":"AACA,SAASA,QAAO,QAAS,KAAK;AAC9B,SAASC,YAAW,QAAS,oBAAoB;AACjD,SAASC,UAAS,QAAS,uBAAuB;AAElD,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAKA,CAAA,EAAG;IACN,MAAMC,SAAQ,GAAIJ,YAAY,CAAC,CAAC;IAEhC,MAAMK,SAAQ,GAAIN,QAAQ,CAAC,MAAM;MAC/B,QAAQK,SAAS,CAACE,QAAQ;QACxB,KAAKL,UAAU,CAACM,aAAa;UAC3B,OAAO,cAAc;QACvB,KAAKN,UAAU,CAACO,WAAW;UACzB,OAAO,cAAc;QACvB;UACE,OAAO,gBAAgB;MAC3B;IACF,CAAC,CAAC;IAEF,MAAMC,WAAU,GAAIV,QAAQ,CAAC,MAAM;MACjC,QAAQK,SAAS,CAACE,QAAQ;QACxB,KAAKL,UAAU,CAACM,aAAa;UAC3B,OAAOG,OAAO,CAAC,yCAAyC,CAAC;QAC3D,KAAKT,UAAU,CAACO,WAAW;UACzB,OAAOE,OAAO,CAAC,uCAAuC,CAAC;QACzD;UACE,OAAOA,OAAO,CAAC,yCAAyC,CAAC;MAC7D;IACF,CAAC,CAAC;IAEF,OAAO;MACLL,SAAS;MACTI;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}