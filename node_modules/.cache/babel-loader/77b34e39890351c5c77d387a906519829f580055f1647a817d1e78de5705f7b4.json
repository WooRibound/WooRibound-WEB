{"ast":null,"code":"import { ref } from 'vue';\nimport MenuGuidePopup from \"@/components/MenuGuidePopup.vue\";\nimport ModalPopup from \"@/components/SingleButtonModal.vue\";\nexport default {\n  name: \"BottomNavigation\",\n  components: {\n    ModalPopup,\n    MenuGuidePopup\n  },\n  setup(props, {\n    emit\n  }) {\n    const menuGuidePopupStatue = ref(false);\n    const icons = {\n      home: require('@/assets/images/icons/home.png'),\n      education: require('@/assets/images/icons/education.png'),\n      company: require('@/assets/images/icons/company.png'),\n      user: require('@/assets/images/icons/user.png')\n    };\n    const hoverIcons = {\n      home: require('@/assets/images/icons/home_hover.png'),\n      education: require('@/assets/images/icons/education_hover.png'),\n      company: require('@/assets/images/icons/company_hover.png'),\n      user: require('@/assets/images/icons/user_hover.png')\n    };\n    const currentIcon = ref({\n      home: true,\n      education: true,\n      company: true,\n      user: true\n    });\n    const showMenuGuide = ref({\n      home: false,\n      education: false,\n      company: false,\n      user: false\n    });\n\n    // 마우스 이벤트 핸들러\n    const mouseEnter = icon => {\n      currentIcon.value[icon] = false; // Hover 시 기본 아이콘 숨김\n    };\n    const mouseLeave = icon => {\n      currentIcon.value[icon] = true; // Hover 해제 시 기본 아이콘 표시\n    };\n    const onShowMenuGuideClick = () => {\n      menuGuidePopupStatue.value = true;\n    };\n    return {\n      menuGuidePopupStatue,\n      icons,\n      hoverIcons,\n      currentIcon,\n      showMenuGuide,\n      mouseEnter,\n      mouseLeave,\n      onShowMenuGuideClick\n    };\n  }\n};","map":{"version":3,"names":["ref","MenuGuidePopup","ModalPopup","name","components","setup","props","emit","menuGuidePopupStatue","icons","home","require","education","company","user","hoverIcons","currentIcon","showMenuGuide","mouseEnter","icon","value","mouseLeave","onShowMenuGuideClick"],"sources":["/Users/sol/Desktop/fisa/wooribound-web/src/components/BottomNavigation.vue"],"sourcesContent":["<script>\nimport { ref } from 'vue';\nimport MenuGuidePopup from \"@/components/MenuGuidePopup.vue\";\nimport ModalPopup from \"@/components/SingleButtonModal.vue\";\n\nexport default {\n  name: \"BottomNavigation\",\n  components: {ModalPopup, MenuGuidePopup},\n  setup(props, { emit }) {\n    const menuGuidePopupStatue = ref(false);\n\n    const icons = {\n      home: require('@/assets/images/icons/home.png'),\n      education: require('@/assets/images/icons/education.png'),\n      company: require('@/assets/images/icons/company.png'),\n      user: require('@/assets/images/icons/user.png')\n    };\n\n    const hoverIcons = {\n      home: require('@/assets/images/icons/home_hover.png'),\n      education: require('@/assets/images/icons/education_hover.png'),\n      company: require('@/assets/images/icons/company_hover.png'),\n      user: require('@/assets/images/icons/user_hover.png')\n    };\n\n    const currentIcon = ref({\n      home: true,\n      education: true,\n      company: true,\n      user: true\n    });\n\n    const showMenuGuide = ref({\n      home: false,\n      education: false,\n      company: false,\n      user: false\n    });\n\n    // 마우스 이벤트 핸들러\n    const mouseEnter = (icon) => {\n      currentIcon.value[icon] = false; // Hover 시 기본 아이콘 숨김\n    };\n\n    const mouseLeave = (icon) => {\n      currentIcon.value[icon] = true; // Hover 해제 시 기본 아이콘 표시\n    };\n\n    const onShowMenuGuideClick = () => {\n      menuGuidePopupStatue.value = true;\n    };\n\n    return {\n      menuGuidePopupStatue,\n      icons,\n      hoverIcons,\n      currentIcon,\n      showMenuGuide,\n      mouseEnter,\n      mouseLeave,\n      onShowMenuGuideClick\n    };\n  }\n}\n</script>\n\n<template>\n  <nav class=\"bottom-navigation\">\n    <ul class=\"bottom-nav-list\">\n      <li class=\"bottom-nav-item\">\n        <router-link to=\"/\"\n                     @mouseenter=\"mouseEnter('home')\"\n                     @mouseleave=\"mouseLeave('home')\"\n        >\n          <img :src=\"currentIcon.home ? icons.home : hoverIcons.home\" alt=\"홈\" class=\"nav-icon\" />\n          홈\n        </router-link>\n      </li>\n      <li class=\"bottom-nav-item\">\n        <router-link to=\"/education/application\"\n                     @mouseenter=\"mouseEnter('education')\"\n                     @mouseleave=\"mouseLeave('education')\"\n        >\n          <img :src=\"currentIcon.education ? icons.education : hoverIcons.education\" alt=\"교육\" class=\"nav-icon\" />\n          교육 신청\n        </router-link>\n      </li>\n      <li class=\"bottom-nav-item\">\n        <router-link to=\"/jobs/menu\"\n                     @mouseenter=\"mouseEnter('company')\"\n                     @mouseleave=\"mouseLeave('company')\"\n        >\n          <img :src=\"currentIcon.company ? icons.company : hoverIcons.company\" alt=\"채용공고\" class=\"nav-icon\" />\n          채용 공고\n        </router-link>\n      </li>\n      <li class=\"bottom-nav-item\">\n        <a href=\"#\"\n           @mouseenter=\"mouseEnter('user')\"\n           @mouseleave=\"mouseLeave('user')\"\n           @click=\"onShowMenuGuideClick\">\n          <img :src=\"currentIcon.user ? icons.user : hoverIcons.user\" alt=\"마이\" class=\"nav-icon\" />\n          마이\n        </a>\n      </li>\n    </ul>\n  </nav>\n  <menu-guide-popup\n      v-if=\"menuGuidePopupStatue\"\n      @close-modal=\"menuGuidePopupStatue = false\"\n  />\n</template>\n\n<style scoped>\n.bottom-navigation {\n  background-color: #fff;\n  padding: 10px;           /* 패딩 추가 */\n  border-top: 2px solid #D9D9D9;\n}\n\n.bottom-nav-list {\n  display: flex;           /* 수평 정렬 */\n  list-style: none;       /* 기본 리스트 스타일 제거 */\n  margin: 0;              /* 기본 마진 제거 */\n  padding: 0 10px;        /* 기본 패딩 제거 */\n  justify-content: space-between;\n}\n\n.bottom-nav-item {\n  text-align: center;      /* 텍스트 가운데 정렬 */\n}\n\n.bottom-nav-item a {\n  text-decoration: none;    /* 링크 기본 스타일 제거 */\n  color: #333;             /* 텍스트 색상 */\n  display: flex;           /* Flexbox 사용 */\n  flex-direction: column;  /* 수직 정렬 */\n  align-items: center;     /* 수평 가운데 정렬 */\n}\n\n.nav-icon {\n  width: 24px;             /* 아이콘 크기 설정 (필요에 따라 조정) */\n  height: 24px;            /* 아이콘 높이 설정 */\n  margin-bottom: 5px;      /* 아이콘과 텍스트 간격 */\n}\n</style>\n"],"mappings":"AACA,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,UAAS,MAAO,oCAAoC;AAE3D,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,UAAU,EAAE;IAACF,UAAU;IAAED;EAAc,CAAC;EACxCI,KAAKA,CAACC,KAAK,EAAE;IAAEC;EAAK,CAAC,EAAE;IACrB,MAAMC,oBAAmB,GAAIR,GAAG,CAAC,KAAK,CAAC;IAEvC,MAAMS,KAAI,GAAI;MACZC,IAAI,EAAEC,OAAO,CAAC,gCAAgC,CAAC;MAC/CC,SAAS,EAAED,OAAO,CAAC,qCAAqC,CAAC;MACzDE,OAAO,EAAEF,OAAO,CAAC,mCAAmC,CAAC;MACrDG,IAAI,EAAEH,OAAO,CAAC,gCAAgC;IAChD,CAAC;IAED,MAAMI,UAAS,GAAI;MACjBL,IAAI,EAAEC,OAAO,CAAC,sCAAsC,CAAC;MACrDC,SAAS,EAAED,OAAO,CAAC,2CAA2C,CAAC;MAC/DE,OAAO,EAAEF,OAAO,CAAC,yCAAyC,CAAC;MAC3DG,IAAI,EAAEH,OAAO,CAAC,sCAAsC;IACtD,CAAC;IAED,MAAMK,WAAU,GAAIhB,GAAG,CAAC;MACtBU,IAAI,EAAE,IAAI;MACVE,SAAS,EAAE,IAAI;MACfC,OAAO,EAAE,IAAI;MACbC,IAAI,EAAE;IACR,CAAC,CAAC;IAEF,MAAMG,aAAY,GAAIjB,GAAG,CAAC;MACxBU,IAAI,EAAE,KAAK;MACXE,SAAS,EAAE,KAAK;MAChBC,OAAO,EAAE,KAAK;MACdC,IAAI,EAAE;IACR,CAAC,CAAC;;IAEF;IACA,MAAMI,UAAS,GAAKC,IAAI,IAAK;MAC3BH,WAAW,CAACI,KAAK,CAACD,IAAI,IAAI,KAAK,EAAE;IACnC,CAAC;IAED,MAAME,UAAS,GAAKF,IAAI,IAAK;MAC3BH,WAAW,CAACI,KAAK,CAACD,IAAI,IAAI,IAAI,EAAE;IAClC,CAAC;IAED,MAAMG,oBAAmB,GAAIA,CAAA,KAAM;MACjCd,oBAAoB,CAACY,KAAI,GAAI,IAAI;IACnC,CAAC;IAED,OAAO;MACLZ,oBAAoB;MACpBC,KAAK;MACLM,UAAU;MACVC,WAAW;MACXC,aAAa;MACbC,UAAU;MACVG,UAAU;MACVC;IACF,CAAC;EACH;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}